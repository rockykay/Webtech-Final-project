#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@isaacs/ts-node-temp-fork-for-pr-2009/dist/bin-cwd.js" "$@"
else 
  exec node  "$basedir/../@isaacs/ts-node-temp-fork-for-pr-2009/dist/bin-cwd.js" "$@"
fi
